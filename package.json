{
  "name": "slack-activator",
  "version": "0.0.0",
  "description": "activate your Slack online status.",
  "main": "src/index.js",
  "repository": "ssh://git@github.com/75asa/slack-activator.git",
  "author": "75asa <75asa.me@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix && prettier --write './{__tests__,src}/**/*.{js,ts}'",
    "start": "node dist/index.js",
    "start:setAuto": "node dist/index.js --mode=auto",
    "start:setAway": "node dist/index.js --mode=away",
    "dev:setAuto": "ts-node -r tsconfig-paths/register --files src/index.ts --mode=auto",
    "dev:setAway": "ts-node -r tsconfig-paths/register --files src/index.ts --mode=away"
  },
  "devDependencies": {
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@slack/web-api": "^6.5.1",
    "dotenv": "^12.0.3",
    "ts-command-line-args": "^2.2.0"
  }
}
